{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": "standard",
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "globals": {
    "$": "readonly",
    "jQuery": "readonly"
  },
  "rules": {
    "quotes": "off",
    "semi": "off",
    "space-before-function-paren": "off",
    "comma-dangle": "off"
  }
}
